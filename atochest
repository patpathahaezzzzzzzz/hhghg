print("Made By [Your Name Here]")

-- Load necessary modules
loadstring(game:HttpGet("https://raw.githubusercontent.com/fdvll/pet-simulator-99/main/waitForGameLoad.lua"))()
loadstring(game:HttpGet("https://raw.githubusercontent.com/fdvll/pet-simulator-99/main/antiStaff.lua"))()

-- Disable idle tracking
game.Players.LocalPlayer.PlayerScripts.Scripts.Core["Idle Tracking"].Enabled = false

-- Prevent character from idling
if getconnections then
    for _, v in pairs(getconnections(game.Players.LocalPlayer.Idled)) do
        v:Disable()
    end
else
    game.Players.LocalPlayer.Idled:Connect(function()
        game:GetService("VirtualUser"):Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
        task.wait(1)
        game:GetService("VirtualUser"):Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    end)
end

-- Teleport to the dig site entrance
game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = game:GetService("Workspace").Digsite.TeleportEntrance.CFrame

-- Define functions to find blocks and chests
local function findBlock()
    -- Your logic to find blocks in the dig site
end

local function findChest()
    -- Your logic to find chests in the dig site
end

-- Main loop to perform actions
while true do
    local chest = findChest()
    local block = findBlock()

    if chest then
        -- Move to the chest and perform action
        game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = chest.CFrame
        -- Your action to interact with the chest
    elseif block then
        -- Move to the block and perform action
        game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = block.CFrame
        -- Your action to interact with the block
    end

    -- Adjust this delay according to your game's mechanics
    task.wait(1)
end
